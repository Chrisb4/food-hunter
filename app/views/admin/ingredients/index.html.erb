<h1>Ingredients</h1>
<br>
<p><%= link_to 'Add Ingredient', new_admin_ingredient_path %></p>
<br>
  <% @ingredients.each do |ingredient| %>

      <p class= 'ingredient_name'><%= ingredient.name %></p>
      <div><%= link_to 'Edit', edit_admin_ingredient_path(ingredient)  %> | <%= link_to 'Delete', admin_ingredient_path(ingredient), method: :delete  %></div>

  <% end %>
