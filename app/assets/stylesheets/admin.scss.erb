 @import url(//fonts.googleapis.com/css?family=Fontdiner+Swanky);
.admin-nav{
  background: #F5F6C8 url(<%= image_path 'vintage-banner.png' %>) no-repeat center;
  border: 1px solid black;
  height: 108px;
    ul {
      margin-top: 30px;
      font-family: 'Fontdiner Swanky', cursive;
      font-size: 1.5em;
      li {
        display: inline;
        margin-left: 40px;
        a:link {
          color: black;
          text-decoration: none;
        }
        a:visited {
          color: black;
          text-decoration: none;
        }
        a:hover {
          color: red;
        }
      }
    }
}





.admin-layout{
  font-family: helvetica;

}
.admin-yield {
  margin: 70px auto;
  width: 60%;
  // box-sizing: border-box;
  border: 1px solid black;
  overflow: auto;

}
